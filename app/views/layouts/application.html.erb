<!DOCTYPE html>
<html>
  <head>
    <title>SellClothes</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-2.1.2.js"></script>
    <script src="//maps.google.com/maps/api/js?v=3.24&key=AIzaSyCzf6dGjKKWh__WIBcxM4q-cgqnLq_xFbE"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
    <script src="https://cdn.jsdelivr.net/elevatezoom/3.0.8/jqueryElevateZoom.js" type="text/javascript"></script>
    <%= csrf_meta_tags %>
  </head>
  <body ng-app="myApp">
    <%= render "layouts/header" %>
    <div class="container">
      <div id="container_body">
        <%= render "shared/flash" %>
      </div>
      <%= yield %>
      <div>
      <%= render "layouts/footer" %>
      </div>
    </div>
  </body>
</html>
